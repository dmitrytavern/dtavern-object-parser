{
	"name": "@dtavern/object-parser",
	"version": "1.1.0",
	"description": "Js object validator",
	"author": {
		"name": "Dmitry Tavern",
		"url": "https://github.com/DmitryTavern"
	},
	"repository": {
		"url": "https://github.com/DmitryTavern/dtavern-object-parser"
	},
	"bugs": {
		"url": "https://github.com/DmitryTavern/dtavern-object-parser/issues"
	},
	"keywords": [
		"object-parser",
		"object-validator",
		"options",
		"options-parser",
		"options-validator",
		"props",
		"props-parser",
		"props-validator"
	],
	"license": "MIT",
	"main": "dist/object-parser.js",
	"module": "dist/object-parser.esm.js",
	"types": "types/index.d.ts",
	"scripts": {
		"dev": "cross-env NODE_ENV=development rollup -w -c rollup.config.js",
		"build": "cross-env NODE_ENV=production rollup -c rollup.config.js",
		"test": "npm run build && jest",
		"prepare": "husky install",
		"lint": "eslint . --ext .ts"
	},
	"devDependencies": {
		"@babel/core": "^7.18.6",
		"@babel/preset-env": "^7.18.6",
		"@rollup/plugin-babel": "^5.3.1",
		"@rollup/plugin-terser": "^0.1.0",
		"@rollup/plugin-typescript": "^8.3.3",
		"@types/jest": "^28.1.3",
		"@types/node": "^18.0.0",
		"@typescript-eslint/eslint-plugin": "^5.30.0",
		"@typescript-eslint/parser": "^5.30.0",
		"cross-env": "^7.0.3",
		"eslint": "^8.18.0",
		"husky": "^8.0.0",
		"jest": "^28.1.1",
		"lint-staged": "^13.0.3",
		"prettier": "^2.7.1",
		"rollup": "^2.75.7",
		"rollup-plugin-delete": "^2.0.0",
		"ts-jest": "^28.0.5",
		"tslib": "^2.4.0",
		"typescript": "^4.7.4"
	},
	"lint-staged": {
		"src/**/*.ts": [
			"eslint",
			"prettier --write"
		],
		"tests/**/*.ts": [
			"eslint",
			"prettier --write"
		],
		"types/**/*.ts": [
			"eslint",
			"prettier --write"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	}
}
