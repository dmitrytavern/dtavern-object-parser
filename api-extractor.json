{
  "projectFolder": ".",

  "compiler": {
    "tsconfigFilePath": "api-extractor.tsconfig.json"
  },

  "mainEntryPointFilePath": "./.temp/index.d.ts",

  "dtsRollup": {
    "enabled": true,
    "untrimmedFilePath": "",
    "publicTrimmedFilePath": "<projectFolder>/dist/<unscopedPackageName>.d.ts"
  },

  "apiReport": {
    "enabled": false
  },

  "docModel": {
    "enabled": false
  },

  "tsdocMetadata": {
    "enabled": false
  },

  "messages": {
    "extractorMessageReporting": {
      "default": {
        "logLevel": "warning"
      },
  
      "ae-forgotten-export": {
        "logLevel": "none"
      }
    },

    "tsdocMessageReporting": {
      "default": {
        "logLevel": "warning"
      },

      "tsdoc-param-tag-missing-hyphen": {
        "logLevel": "none"
      }
    }
  }
}
